{
  "name": "js-library-template",
  "author": "Justin Perry",
  "version": "0.0.0",
  "description": "Base config for a JavaScript library. Includes Rollup, Prettier, ESLint, Babel and Flow",
  "repository": "js-library-template",
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "jest --watchAll --env=jsdom",
    "test:ci": "jest --ci",
    "prepare": "yarn run build",
    "build": "rollup -c",
    "start": "rollup -c -w"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "dependencies": {
    "flow-typed": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "browserslist": "^3.2.8",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^4.0.0",
    "flow-bin": "0.75.0",
    "jest": "^24.8.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-url": "^2.2.1"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
